{
  "pages/custom/page/ob-location-denver": "{{#partial \"page\"}}\r\n\r\n<style>\r\n    .body {\r\n        margin: 0;\r\n    }\r\n    .body > .container {\r\n        max-width: 100%;\r\n        padding: 0;\r\n    }\r\n</style>\r\n\r\n<main class=\"main full ob-location ob-active-location\">\r\n    <div class=\"location-hero\">\r\n        <iframe src=\"https://www.google.com/maps/embed/v1/place?key=AIzaSyCcywKcxXeMZiMwLDcLgyEnNglcLOyB_qw&q=8101 Belleview Ave #w-2 Denver, CO 80237&zoom=15\" style=\"display:block\"></iframe>\r\n\r\n        <div class=\"bg-overlay\">\r\n            <div class=\"container\">\r\n                <h1 class=\"page-heading ob-heading-xl ob-text-white\">Denver Tech Center</h1>\r\n\r\n                <div class=\"btn-block\">\r\n                    <a href=\"../meals/\" class=\"ob-btn ob-btn-light-green ob-btn-inline\">\r\n                        Order from DTC \r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-arrow-lr-blue\"></use>\r\n                        </svg>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <div class=\"location-content\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-md-6 col-sm-6\">\r\n                    <h2 class=\"ob-heading-lg\">\r\n                        HELLO,<br />\r\n                        MyFitFoods @Denver Tech Center\r\n                    </h2>\r\n\r\n                    <p class=\"ob-paragraph-md\">\r\n                        Come visit us at our Denver location! Our friendly team of nutritionists and health enthusiasts are ready to help you reach your goals with fresh, tasty, healthy meals!\r\n                    </p>\r\n\r\n                    <div class=\"location-contact\">\r\n                        <div class=\"row\">\r\n                            <div class=\"col-md-6\">\r\n                                <h3 class=\"ob-heading-sm ob-text-orange\">Address</h3>\r\n                                <p>\r\n                                    Denver Tech Center<br />\r\n                                    8101 Belleview Ave #W-2<br />\r\n                                    Denver, CO 80237\r\n                                </p>\r\n                            </div>\r\n                            <div class=\"col-md-6\">\r\n                                <h3 class=\"ob-heading-sm ob-text-orange\">Hours</h3>\r\n                                <p>\r\n                                    Monday-Thursday: 7am-7pm<br />\r\n                                    Friday: 8am-6pm<br />\r\n                                    Saturday: 9am-6pm<br />\r\n                                    Sunday: 10am-8pm\r\n                                </p>\r\n                            </div>\r\n                            <div class=\"col-md-6\">\r\n                                <h3 class=\"ob-heading-sm ob-text-orange\">Call Us</h3>\r\n                                <p>(303) 942-1514</p>\r\n                            </div>\r\n                            <div class=\"col-md-6\">\r\n                                <h3 class=\"ob-heading-sm ob-text-orange\">Email Us</h3>\r\n                                <p><a href=\"mailto:dtc@myfitfoods.com\" target=\"_blank\" rel=\"nofollow\" style=\"\" class=\"brz-cp-color8 link--external\" data-brz-link-type=\"external\">Click here to email us</a></p>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"col-md-6 col-sm-6 location-map\">\r\n                    <iframe class=\"brz-iframe intrinsic-ignore\" src=\"https://www.google.com/maps/embed/v1/place?key=AIzaSyCcywKcxXeMZiMwLDcLgyEnNglcLOyB_qw&q=8101 Belleview Ave #w-2 Denver, CO 80237&zoom=15\"></iframe>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</main>\r\n\r\n{{/partial}}\r\n\r\n{{> layout/base}}\r\n",
  "layout/base": "<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n\r\n        {{> components/common/polyfill-script }}\r\n\r\n        {{!-- Load Lazysizes script ASAP so images will appear --}}\r\n        <script>\r\n            {{!-- Only load visible elements until the onload event fires, after which preload nearby elements. --}}\r\n            window.lazySizesConfig = window.lazySizesConfig || {};\r\n            window.lazySizesConfig.loadMode = 1;\r\n        </script>\r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.head_async.js'}}\"></script>\r\n\r\n        <script src=\"https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js\"></script>\r\n\r\n        <script>\r\n            WebFont.load({\r\n                custom: {\r\n                    families: ['Karla', 'Roboto', 'Source Sans Pro']\r\n                },\r\n                classes: false\r\n            });\r\n        </script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n\r\n        {{~inject 'storefrontAPIToken' settings.storefront_api.token}}\r\n        {{~inject 'zoomSize' theme_settings.zoom_size}}\r\n        {{~inject 'productSize' theme_settings.product_size}}\r\n        {{~inject 'showAdminBar' theme_settings.show-admin-bar}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceModeSettings' settings.maintenance}}\r\n        {{~inject 'adminBarLanguage' (langJson 'admin')}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'channelId' settings.channel_id}}\r\n        {{~inject 'template' template}}\r\n        {{~inject 'validationDictionaryJSON' (langJson 'validation_messages')}}\r\n        {{~inject 'validationFallbackDictionaryJSON' (langJson 'validation_fallback_messages')}}\r\n        {{~inject 'validationDefaultDictionaryJSON' (langJson 'default_messages')}} \r\n    </head>\r\n    <body>\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{> components/ob-custom/ob-header }}\r\n        {{> components/common/body }}\r\n        {{> components/ob-custom/ob-footer }}\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n\r\n        {{{footer.scripts}}}\r\n    </body>\r\n</html>\r\n",
  "components/common/polyfill-script": "<script>\r\n    {{!--\r\n        Check for modern browser features, and load polyfills if browser does not appear to support features\r\n        we need.\r\n\r\n            Polyfill approach is all or nothing - please import needed polyfills in /assets/js/polyfills.js\r\n\r\n        Inspired by https://philipwalton.com/articles/loading-polyfills-only-when-needed/\r\n            --}}\r\n    function browserSupportsAllFeatures() {\r\n        return window.Promise\r\n            && window.fetch\r\n            && window.URL\r\n            && window.URLSearchParams\r\n            && window.WeakMap\r\n            // object-fit support\r\n            && ('objectFit' in document.documentElement.style);\r\n    }\r\n\r\n    function loadScript(src) {\r\n        var js = document.createElement('script');\r\n        js.src = src;\r\n        js.onerror = function () {\r\n            console.error('Failed to load polyfill script ' + src);\r\n        };\r\n        document.head.appendChild(js);\r\n    }\r\n\r\n    if (!browserSupportsAllFeatures()) {\r\n        loadScript('{{cdn 'assets/dist/theme-bundle.polyfills.js'}}');\r\n    }\r\n</script>\r\n",
  "components/ob-custom/ob-header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n<header class=\"header\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n    </a>\r\n\r\n    <div class=\"container header-container\">\r\n        {{#if page_type '===' 'default'}}\r\n            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                <a href=\"/\"><img src=\"{{cdn 'assets/img/ob-custom/MFF_Website_Files_Logo_Nav.png'}}\" alt=\"\" /></a>\r\n            </h1>\r\n        {{else}}\r\n            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                <a href=\"/\"><img src=\"{{cdn 'assets/img/ob-custom/MFF_Website_Files_Logo_Nav.png'}}\" alt=\"\" /></a>\r\n            </div>\r\n        {{/if}}\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/ob-custom/ob-navigation-menu}}\r\n        </div>\r\n        <div class=\"navPages-quickSearch ob-desktop\">\r\n            {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n        </div>\r\n    </div>\r\n</header>\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n\r\n<a href=\"\" class=\"skip-to-top\">\r\n    <svg>\r\n        <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-arrow-up\"></use>\r\n    </svg>\r\n</a>\r\n",
  "components/common/body": "<main class=\"body\" id='main-content' role='main' data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    <div class=\"container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n</main>\r\n",
  "components/ob-custom/ob-footer": "{{#if banners.bottom}}\r\n    {{#if settings.data_tag_enabled}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            {{#each (limit banners.bottom_metadata 1)}}\r\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                    <div data-event-type=\"promotion-click\">\r\n                        {{{this.content}}}\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            <div class=\"banner\">\r\n                {{{limit banners.bottom 1}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        <section class=\"footer-info row\">\r\n            <div class=\"col-md-8 col-sm-12\">\r\n                <article class=\"footer-info-col col-md-4 col-sm-4\" data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">About</h3>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li>\r\n                            <a href=\"/why-mff/\">About MyFitFoods</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/success-stories/\">Real Stories</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/locations/\">Locations</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/fit-club/\">Fit Club</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/faq/\">FAQs</a>\r\n                        </li>\r\n                    </ul>\r\n                </article>\r\n                <article class=\"footer-info-col col-md-4 col-sm-4\" data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">Foods</h3>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li>\r\n                            <a href=\"/quickstart/\">Shop All Meals</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/breakfast/\">Breakfast</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/lunch/\">Lunch</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/dinner/\">Dinner</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/snacks/\">Snacks</a>\r\n                        </li>\r\n                    </ul>\r\n                </article>\r\n                <article class=\"footer-info-col col-md-4 col-sm-4\" data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">Info</h3>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li>\r\n                            <a href=\"/blog/\">The Fit Life Blog</a>\r\n                        </li>\r\n                        <!--<li>-->\r\n                        <!--    <a href=\"/brand-assets/\">Brand Assets</a>-->\r\n                        <!--</li>-->\r\n                        <li>\r\n                            <a href=\"/contact-us/\">Contact Us</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/fitchecks/\">#Fitchecks</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                        </li>\r\n                    </ul>\r\n                </article>\r\n            </div>\r\n            <div class=\"col-md-4 col-sm-12\">\r\n                <article class=\"footer-info-col footer-social-col col-md-12\">\r\n                    <img src=\"{{cdn 'assets/img/ob-custom/logo_footer.png'}}\" class=\"sub-logo\" alt=\"\" />\r\n\r\n                    <div class=\"social-block\">\r\n                        <a href=\"https://www.facebook.com/MyFitFoodsMeals\"><img src=\"{{cdn 'assets/img/ob-custom/facebook.png'}}\" alt=\"\" style=\"filter: hue-rotate(\r\n68deg\r\n) saturate(0.7);\"/></a>\r\n                        <a href=\"https://www.instagram.com/myfitfoodsofficial/\"><img src=\"{{cdn 'assets/img/ob-custom/instagram.png'}}\" alt=\"\" style=\"filter: hue-rotate(\r\n68deg\r\n) saturate(0.7);\"/></a>\r\n                        <span>#GIVEAFIT</span>\r\n                    </div>\r\n                </article>\r\n            </div>\r\n        </section>\r\n\r\n        <div class=\"footer-copyright\">\r\n            <div class=\"container\">\r\n                <div class=\"privacy-links\">\r\n                    <a href=\"/privacy-policy/\">Privacy Policy</a>\r\n                    <a href=\"/terms-of-service/\">Terms of Service</a>\r\n                </div>\r\n\r\n                <div class=\"footer-store\">\r\n                    <p>&copy; {{moment format=\"YYYY\"}} MyFitFoods, LLC.</p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</footer>\r\n",
  "components/ob-custom/ob-navigation-menu": "<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch ob-mobile\">\r\n      {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action\"\r\n               href=\"/meals\"\r\n               aria-label=\"Shop\">\r\n                Shop\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Why MFF?' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/why-mff\"\r\n               aria-label=\"Why MFF?\">\r\n                Why MFF?\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Fit Club' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/fit-club\"\r\n               aria-label=\"Fit Club\">\r\n                Fit Club\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if '21 Days' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/myfitlife\"\r\n               aria-label=\"21 Days\">\r\n                MyFitLife\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Locations' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/locations\"\r\n               aria-label=\"Locations\">\r\n                Locations\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Blog' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/blog\"\r\n               aria-label=\"Blog\">\r\n                Blog\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page navPages-item-icon\">\r\n            {{#if customer}}\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   aria-label=\"{{lang 'common.account'}}\"\r\n                >\r\n                 <svg><use xlink:href=\"#icon-account\" /></svg>\r\n                </a>\r\n            {{else}}\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                <svg class=\"ob-desktop\"><use xlink:href=\"#icon-account\" /></svg>\r\n                <span class=\"ob-mobile\">Account</span>\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page navPages-item-icon\">\r\n            <a class=\"navPages-action\"\r\n               href=\"{{urls.cart}}\"\r\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n                <span class=\"navUser-item--sub navUser-item-cartLabel ob-desktop\">          \r\n                  <svg><use xlink:href=\"#icon-ob-basket\" /></svg>              \r\n                </span>\r\n                <span class=\"navUser-item--sub full-cart-qty ob-desktop\">(<span class=\"countPill cart-quantity\"></span>)</span>\r\n                <span class=\"ob-mobile\">\r\n                  View Cart\r\n                  <span class=\"navUser-item--sub full-cart-qty\">(<span class=\"countPill cart-quantity\"></span>)</span>\r\n                </span>\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page navPages-item-search navPages-item-icon\">\r\n          <a class=\"navPages-action ob-search-toggle\">\r\n            <svg>\r\n              <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-search\"></use>\r\n            </svg>\r\n          </a>\r\n        </li>\r\n    </ul>\r\n</nav>\r\n",
  "components/common/quick-search": "<div class=\"container\">\r\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                       data-search-quick\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <span class=\"exit\">x</span>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n    <p role=\"status\"\r\n       aria-live=\"polite\"\r\n       class=\"aria-description--hidden\"\r\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\r\n    ></p>\r\n</div>\r\n",
  "components/common/modal/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    {{> components/common/modal/modal-close-btn}}\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n",
  "components/common/alert/alert-modal": "<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"swal2-icon swal2-error swal2-icon-show\"><span class=\"swal2-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\r\n</div>\r\n",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\r\n        type=\"button\"\r\n        title=\"{{lang 'common.close'}}\"\r\n        {{additional_attr}}\r\n>\r\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\r\n    <span aria-hidden=\"true\">&#215;</span>\r\n</button>\r\n"
}
