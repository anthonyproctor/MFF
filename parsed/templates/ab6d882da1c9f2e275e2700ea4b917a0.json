  "components/ob-custom/locations/locations-roadmap": "<div class=\"roadmap\">\r\n\t<div class=\"container\">\r\n\t\t<h2 class=\"ob-heading-lg\">LOCATIONS ON THE ROADMAP</h2>\r\n\t\t<div class=\"row\">\r\n\t\t\t<div class=\"col-md-4 col-sm-4 coming-soon\">\r\n\t\t\t\t<p>HOUSTON – 2021</p>\r\n\t\t\t\t<p>DALLAS (DFW)</p>\r\n\t\t\t\t<p>AUSTIN</p>\r\n\t\t\t\t<p>PHOENIX</p>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-md-4 col-sm-4 coming-soon\">\r\n\t\t\t\t<p>COLORADO SPRINGS</p>\r\n\t\t\t\t<p>SALT LAKE CITY</p>\r\n\t\t\t\t<p>FORT COLLINS</p>\r\n\t\t\t\t<p>SEATTLE</p>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-md-4 col-sm-4 coming-soon\">\r\n\t\t\t\t<p>SAN FRANCISCO</p>\r\n\t\t\t\t<p>LOS ANGELES</p>\r\n\t\t\t\t<p>YOUR CITY !</p>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\r\n\t\t{{> components/ob-custom/locations/ob-newsletter}}\r\n\t</div>\r\n</div>\n",
  "components/ob-custom/locations/ob-newsletter": "<div class=\"ob-content-panel ob-newsletter-panel locations-newsletter\">\r\n\t<div class=\"container\">\r\n\t\t<h2 class=\"ob-heading-lg ob-text-blue\">WANT TO KNOW WHEN WE’VE ARRIVED?</h2>\r\n\t\t<p class=\"ob-paragraph-lg ob-text-blue\">Get exclusive deals at new and upcoming locations too!</p>\r\n\t\t<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n\t\t    <fieldset class=\"form-fieldset\">\r\n\t\t        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n\t\t        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n\t\t        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n\t\t        <div class=\"form-field\">\r\n\t\t            <div class=\"form-prefixPostfix wrap\">\r\n\t\t                <input class=\"form-input\"\r\n\t\t                       id=\"nl_email\"\r\n\t\t                       name=\"nl_email\"\r\n\t\t                       type=\"email\"\r\n\t\t                       value=\"{{customer.email}}\"\r\n\t\t                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n\t\t                       aria-describedby=\"alertBox-message-text\"\r\n\t\t                >\r\n\t\t                <input class=\"button button--primary ob-btn-blue form-prefixPostfix-button--postfix\"\r\n\t\t                       type=\"submit\"\r\n\t\t                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\r\n\t\t                >\r\n\t\t            </div>\r\n\t\t            {{#if settings.show_newsletter_summary }}\r\n\t\t                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n\t\t            {{/if}}\r\n\t\t        </div>\r\n\t\t    </fieldset>\r\n\t\t</form>\r\n\t\t<p class=\"ob-paragraph-sm sub-notice\">We’ll never share your personal information because that’s not cool.</p>\r\n\t</div>\r\n</div>\n"
