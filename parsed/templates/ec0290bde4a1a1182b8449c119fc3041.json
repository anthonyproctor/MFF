{
  "pages/home": "---\r\nproducts:\r\n    new:\r\n        limit: {{theme_settings.homepage_new_products_count}}\r\n    featured:\r\n        limit: {{theme_settings.homepage_featured_products_count}}\r\n    top_sellers:\r\n        limit: {{theme_settings.homepage_top_products_count}}\r\ncarousel: {{theme_settings.homepage_show_carousel}}\r\nblog:\r\n    recent_posts:\r\n        limit: {{theme_settings.homepage_blog_posts_count}}\r\n---\r\n\r\n{{#partial \"page\"}}\r\n\r\n{{#each shipping_messages}}\r\n    {{> components/common/alert/alert-info message}}\r\n{{/each}}\r\n\r\n<div class=\"main full home\">\r\n    {{> components/ob-custom/home/ob-home-content}}\r\n</div>\r\n{{/partial}}\r\n{{> layout/base}}\r\n",
  "layout/base": "<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n\r\n        {{> components/common/polyfill-script }}\r\n\r\n        {{!-- Load Lazysizes script ASAP so images will appear --}}\r\n        <script>\r\n            {{!-- Only load visible elements until the onload event fires, after which preload nearby elements. --}}\r\n            window.lazySizesConfig = window.lazySizesConfig || {};\r\n            window.lazySizesConfig.loadMode = 1;\r\n        </script>\r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.head_async.js'}}\"></script>\r\n\r\n        <script src=\"https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js\"></script>\r\n\r\n        <script>\r\n            WebFont.load({\r\n                custom: {\r\n                    families: ['Karla', 'Roboto', 'Source Sans Pro']\r\n                },\r\n                classes: false\r\n            });\r\n        </script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n\r\n        {{~inject 'storefrontAPIToken' settings.storefront_api.token}}\r\n        {{~inject 'zoomSize' theme_settings.zoom_size}}\r\n        {{~inject 'productSize' theme_settings.product_size}}\r\n        {{~inject 'showAdminBar' theme_settings.show-admin-bar}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceModeSettings' settings.maintenance}}\r\n        {{~inject 'adminBarLanguage' (langJson 'admin')}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'channelId' settings.channel_id}}\r\n        {{~inject 'template' template}}\r\n        {{~inject 'validationDictionaryJSON' (langJson 'validation_messages')}}\r\n        {{~inject 'validationFallbackDictionaryJSON' (langJson 'validation_fallback_messages')}}\r\n        {{~inject 'validationDefaultDictionaryJSON' (langJson 'default_messages')}} \r\n    </head>\r\n    <body>\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{> components/ob-custom/ob-header }}\r\n        {{> components/common/body }}\r\n        {{> components/ob-custom/ob-footer }}\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n\r\n        {{{footer.scripts}}}\r\n    </body>\r\n</html>\r\n",
  "components/common/alert/alert-info": "<div class=\"alertBox alertBox--info\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n",
  "components/ob-custom/home/ob-home-content": "<style>\r\n\t.body {\r\n\t\tmargin: 0;\r\n\t}\r\n\t.body .container {\r\n\t\tmax-width: 100%;\r\n\t\tpadding: 0;\r\n\t}\r\n\t.body .container .container {\r\n\t\tmax-width: 1210px;\r\n\t}\r\n</style>\r\n\r\n{{> components/ob-custom/home/ob-hero-banner}}\r\n{{> components/ob-custom/home/ob-covid-banner}}\r\n{{> components/ob-custom/home/ob-choices-panel}}\r\n{{> components/ob-custom/home/ob-quality-panel}}\r\n{{> components/ob-custom/home/ob-21-days-panel}}\r\n{{> components/ob-custom/home/ob-testimonials-panel}}\r\n{{> components/ob-custom/home/ob-popular-panel}}\r\n{{> components/ob-custom/home/ob-new-meal-panel}}\r\n{{> components/ob-custom/home/ob-commitment-panel}}\r\n{{> components/ob-custom/home/ob-social-embed}}\r\n\r\n{{> components/ob-custom/home/ob-newsletter-panel}}\r\n",
  "components/ob-custom/ob-header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n<header class=\"header\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n    </a>\r\n\r\n    <div class=\"container header-container\">\r\n        {{#if page_type '===' 'default'}}\r\n            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                <a href=\"/\"><img src=\"{{cdn 'assets/img/ob-custom/MFF_Website_Files_Logo_Nav.png'}}\" alt=\"\" /></a>\r\n            </h1>\r\n        {{else}}\r\n            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                <a href=\"/\"><img src=\"{{cdn 'assets/img/ob-custom/MFF_Website_Files_Logo_Nav.png'}}\" alt=\"\" /></a>\r\n            </div>\r\n        {{/if}}\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/ob-custom/ob-navigation-menu}}\r\n        </div>\r\n        <div class=\"navPages-quickSearch ob-desktop\">\r\n            {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n        </div>\r\n    </div>\r\n</header>\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n\r\n<a href=\"\" class=\"skip-to-top\">\r\n    <svg>\r\n        <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-arrow-up\"></use>\r\n    </svg>\r\n</a>\r\n",
  "components/common/body": "<main class=\"body\" id='main-content' role='main' data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    <div class=\"container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n</main>\r\n",
  "components/common/polyfill-script": "<script>\r\n    {{!--\r\n        Check for modern browser features, and load polyfills if browser does not appear to support features\r\n        we need.\r\n\r\n            Polyfill approach is all or nothing - please import needed polyfills in /assets/js/polyfills.js\r\n\r\n        Inspired by https://philipwalton.com/articles/loading-polyfills-only-when-needed/\r\n            --}}\r\n    function browserSupportsAllFeatures() {\r\n        return window.Promise\r\n            && window.fetch\r\n            && window.URL\r\n            && window.URLSearchParams\r\n            && window.WeakMap\r\n            // object-fit support\r\n            && ('objectFit' in document.documentElement.style);\r\n    }\r\n\r\n    function loadScript(src) {\r\n        var js = document.createElement('script');\r\n        js.src = src;\r\n        js.onerror = function () {\r\n            console.error('Failed to load polyfill script ' + src);\r\n        };\r\n        document.head.appendChild(js);\r\n    }\r\n\r\n    if (!browserSupportsAllFeatures()) {\r\n        loadScript('{{cdn 'assets/dist/theme-bundle.polyfills.js'}}');\r\n    }\r\n</script>\r\n",
  "components/ob-custom/ob-footer": "{{#if banners.bottom}}\r\n    {{#if settings.data_tag_enabled}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            {{#each (limit banners.bottom_metadata 1)}}\r\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                    <div data-event-type=\"promotion-click\">\r\n                        {{{this.content}}}\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            <div class=\"banner\">\r\n                {{{limit banners.bottom 1}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        <section class=\"footer-info row\">\r\n            <div class=\"col-md-8 col-sm-12\">\r\n                <article class=\"footer-info-col col-md-4 col-sm-4\" data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">About</h3>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li>\r\n                            <a href=\"/why-mff/\">About MyFitFoods</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/success-stories/\">Real Stories</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/locations/\">Locations</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/fit-club/\">Fit Club</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/faq/\">FAQs</a>\r\n                        </li>\r\n                    </ul>\r\n                </article>\r\n                <article class=\"footer-info-col col-md-4 col-sm-4\" data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">Foods</h3>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li>\r\n                            <a href=\"/quickstart/\">Shop All Meals</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/breakfast/\">Breakfast</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/lunch/\">Lunch</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/dinner/\">Dinner</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/snacks/\">Snacks</a>\r\n                        </li>\r\n                    </ul>\r\n                </article>\r\n                <article class=\"footer-info-col col-md-4 col-sm-4\" data-section-type=\"footer-webPages\">\r\n                    <h3 class=\"footer-info-heading\">Info</h3>\r\n                    <ul class=\"footer-info-list\">\r\n                        <li>\r\n                            <a href=\"/blog/\">The Fit Life Blog</a>\r\n                        </li>\r\n                        <!--<li>-->\r\n                        <!--    <a href=\"/brand-assets/\">Brand Assets</a>-->\r\n                        <!--</li>-->\r\n                        <li>\r\n                            <a href=\"/contact-us/\">Contact Us</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/fitchecks/\">#Fitchecks</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n                        </li>\r\n                    </ul>\r\n                </article>\r\n            </div>\r\n            <div class=\"col-md-4 col-sm-12\">\r\n                <article class=\"footer-info-col footer-social-col col-md-12\">\r\n                    <img src=\"{{cdn 'assets/img/ob-custom/logo_footer.png'}}\" class=\"sub-logo\" alt=\"\" />\r\n\r\n                    <div class=\"social-block\">\r\n                        <a href=\"https://www.facebook.com/MyFitFoodsMeals\"><img src=\"{{cdn 'assets/img/ob-custom/facebook.png'}}\" alt=\"\" style=\"filter: hue-rotate(\r\n68deg\r\n) saturate(0.7);\"/></a>\r\n                        <a href=\"https://www.instagram.com/myfitfoodsofficial/\"><img src=\"{{cdn 'assets/img/ob-custom/instagram.png'}}\" alt=\"\" style=\"filter: hue-rotate(\r\n68deg\r\n) saturate(0.7);\"/></a>\r\n                        <span>#GIVEAFIT</span>\r\n                    </div>\r\n                </article>\r\n            </div>\r\n        </section>\r\n\r\n        <div class=\"footer-copyright\">\r\n            <div class=\"container\">\r\n                <div class=\"privacy-links\">\r\n                    <a href=\"/privacy-policy/\">Privacy Policy</a>\r\n                    <a href=\"/terms-of-service/\">Terms of Service</a>\r\n                </div>\r\n\r\n                <div class=\"footer-store\">\r\n                    <p>&copy; {{moment format=\"YYYY\"}} MyFitFoods, LLC.</p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</footer>\r\n",
  "components/ob-custom/home/ob-hero-banner": "<style>\r\n.ob-heading-xl{color:white;}\r\n.ob-paragraph-lg{color:white;}\r\n.ob-btn-blue svg {\r\n    fill: white;\r\n}\r\n@media screen and (max-width:551px) {\r\n.ob-heading-xl{color:#005677;}\r\n.ob-paragraph-lg{color:#005677;}\r\n}\r\n\r\n</style>\r\n<div class=\"ob-hero-banner\">\r\n\t<div class=\"bg-overlay\">\r\n\t\t<div class=\"container\">\r\n\t\t\t<div class=\"panel-content\">\r\n\t\t\t\t<h1 class=\"ob-heading-xl\">FRESH MEALS FOR PEOPLE WHO LOVE HEALTHY FOOD</h1>\r\n\t\t\t\t<p class=\"ob-paragraph-lg\">\r\n\t\t\t\t\tWhy meal prep?  Our 48 delicious meals will save you time and money.\r\n\t\t\t\t</p>\r\n\t\t\t\t<div class=\"fit-meals-wrapper\">\r\n\t\t\t\t\t<a class=\"ob-btn ob-btn-blue ob-btn-inline\" href=\"../meals/\" style=\"color:#fa8a00!important; \">\r\n\t\t\t\t\t\t<span style=\"color:white\">Shop Fit Meals</span>\r\n\t\t\t\t\t\t<svg>\r\n\t\t\t                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-cutlery\"></use>\r\n\t\t\t            </svg>\r\n\t\t\t\t\t</a>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-covid-banner": "<div class=\"ob-covid-banner\">\r\n\t<div class=\"container\">\r\n\t\t<p class=\"ob-paragraph-md ob-text-white\">\r\n\t\t\tRead our <a href=\"/covid-19/\" class=\"ob-paragraph-md\">COVID-19 update page</a> to learn how we're keeping out customers\r\n\t\t\t&amp; FitFam safe.\r\n\t\t</p>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-choices-panel": "<div class=\"ob-content-panel ob-content-panel-75 ob-choices-panel\">\r\n\t<div class=\"container\">\r\n\t\t<div class=\"row\">\r\n\t\t\t<div class=\"col-md-12\">\r\n\t\t\t\t<h2 class=\"ob-heading-lg\">GOOD CHOICES MADE EASY</h2>\r\n\t\t\t</div>\r\n\r\n\t\t\t<div class=\"panel-content-block col-md-6 col-sm-6\">\r\n\t\t\t\t<svg>\r\n\t                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-basket-orange\"></use>\r\n\t            </svg>\r\n\t\t\t\t<h3 class=\"ob-heading-md\">Best-In-Class Ingredients</h3>\r\n\t\t\t\t<p class=\"ob-paragraph-md\">\r\n\t\t\t\t\tWe stock fresh meals daily for all those grab-n-go needs. \r\n\t\t\t\t\tStop by a local store to find the best meals, and  fuel your body the right way today.\r\n\t\t\t\t</p>\r\n\t\t\t\t<a class=\"ob-btn ob-btn-orange ob-btn-inline\" href=\"../locations/\">SHOP YOUR LOCAL STORE</a>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"panel-content-block col-md-6 col-sm-6\">\r\n\t\t\t\t<svg>\r\n\t                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-cutlery-orange\"></use>\r\n\t            </svg>\r\n\t\t\t\t<h3 class=\"ob-heading-md\">BUY ONE MEAL OR ONE WEEK’S WORTH</h3>\r\n\t\t\t\t<p class=\"ob-paragraph-md\">\r\n\t\t\t\t\tMeal planning doesn’t have to be hard. Find the food you want and pick up in-store today or have it delivered tomorrow.  \r\n\t\t\t\t\tAll you do is heat and eat.\r\n\t\t\t\t</p>\r\n\t\t\t\t<a class=\"ob-btn ob-btn-orange ob-btn-inline\" href=\"../meals/\">FIND YOUR FIT LIFESTYLE</a>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-quality-panel": "<div class=\"ob-quality-panel\">\r\n\t<div class=\"bg-overlay\" style=\"background: rgb(0,88,119);\r\nbackground: linear-gradient(315deg, rgba(0,88,119,1) 0%, rgba(46,179,118,1) 100%);\">\r\n\t\t<div class=\"container\">\r\n\t\t\t<div class=\"row\">\r\n\t\t\t\t<div class=\"col-md-6 col-sm-6 image-col\">\r\n\t\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/home-taco-tray.png'}}\" alt=\"\" />\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"col-md-6 col-sm-6\">\r\n\t\t\t\t\t<h3 class=\"ob-heading-sm\"><span style=\"background-color:#fa8a00;padding: 7px; font-weight: 800; font-size: 26px;\">BEST IN CLASS INGREDIENTS</span></h3>\r\n\t\t\t\t\t<p class=\"ob-paragraph-md ob-text-white\">\r\n\t\t\t\t\t\tWe do our best to abstain from added preservatives, fake colors, sugars, and sodium. \r\n\t\t\t\t\t\tOur carbs are gluten free (95% of them) and low on the glycemic index. Proteins are 85% or leaner. Fats are 100% organic and heart healthy.  \r\n\t\t\t\t\t\tAll spices are freshly ground, and have no added sugars, salt, colors or preservatives.  Real food for real people!\r\n\t\t\t\t\t</p>\r\n\t\t\t\t\t<h3 class=\"ob-heading-sm\"><span style=\"background-color:#fa8a00;padding: 7px; font-weight: 800; font-size: 26px;\">COOKED FRESH DAILY</span></h3>\r\n\t\t\t\t\t<p class=\"ob-paragraph-md ob-text-white\">Feel the difference whole food makes. We cook our meals fresh every single day just for you!</p>\r\n\t\t\t\t\t<h3 class=\"ob-heading-sm\"><span style=\"background-color:#fa8a00;padding: 7px; font-weight: 800; font-size: 26px;\">OPTIMIZED RESULTS</span></h3>\r\n\t\t\t\t\t<p class=\"ob-paragraph-md ob-text-white\">\r\n\t\t\t\t\t\tMost people don’t know how many calories they need daily - Do you? \r\n\t\t\t\t\t\tOptimize your energy, hormone, body fat, and lean body mass levels by finding out today. \r\n\t\t\t\t\t\tOur Nutritional Coaches and Dietitians will dial you in, and then explain to you exactly how many \r\n\t\t\t\t\t\tcalories you need daily to reach your goals.\r\n\t\t\t\t\t</p>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-21-days-panel": "<div class=\"ob-content-panel ob-content-panel-75 ob-21-days-panel\">\r\n\t<div class=\"container\">\r\n\t\t<div class=\"row\">\r\n\t\t\t<div class=\"col-md-6 col-sm-6\">\r\n\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/home-team.jpg'}}\" alt=\"\" style=\"    box-shadow: -15px 15px #fa8a00\"/>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-md-6 col-sm-6\">\r\n\t\t\t\t<h2 class=\"ob-heading-lg\">CHANGE YOUR HABITS WITH MYFITLIFE</h2>\r\n\t\t\t\t<p><strong>The meal plan that started it all. </strong>Our legendary My Fit Life plan takes all of the guesswork out of eating right.\r\n\t\t\t\t</p>\r\n\t\t\t\t<div class=\"btn-block\">\r\n\t\t\t\t\t<a class=\"ob-btn ob-btn-light-green\" href=\"../21-day-challenge/\">Get Started</a>\r\n\t\t\t\t\t<a class=\"ob-btn ob-btn-orange\" href=\"../success-stories/\">Real Stories</a>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-testimonials-panel": "<div class=\"ob-content-panel ob-content-panel-75 ob-testimonials-panel\" style=\"background:#044465\">\r\n\t<div class=\"container\">\r\n\t\t<div class=\"quotes-container\">\r\n\t\t\t<div class=\"quote\">\r\n\t\t\t\t<p class=\"ob-text-white\">\r\n\t\t\t\t\t“Being a mom of two and a teacher makes MyFitFoods a lifesaver. No more spending rare free time meal \r\n\t\t\t\t\tprepping for hours on end every Sunday.”\r\n\t\t\t\t</p>\r\n\t\t\t\t<div class=\"source\">\r\n\t\t\t\t\t<span class=\"name\" style=\"color:orange\">Cassidy M.</span>\r\n\t\t\t\t\t<span class=\"location\" style=\"color:white\">Houston, TX</span>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"quote\">\r\n\t\t\t\t<p class=\"ob-text-white\">\r\n\t\t\t\t\t“Feeling the best I’ve ever felt. This program is exactly what I needed to make real change.”\r\n\t\t\t\t</p>\r\n\t\t\t\t<div class=\"source\">\r\n\t\t\t\t\t<span class=\"name\" style=\"color:orange\">Marshall W.</span>\r\n\t\t\t\t\t<span class=\"location\" style=\"color:white\">Houston, TX</span>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-popular-panel": "<div class=\"ob-content-panel ob-popular-panel\">\r\n\t<div class=\"container\">\r\n\t\t<div class=\"row\">\r\n\t\t\t<div class=\"col-md-6 col-sm-6 text-col\">\r\n\t\t\t\t<h4 class=\"ob-heading-sm\">Popular Meals</h4>\r\n\t\t\t\t<h2 class=\"ob-heading-lg\">OUR MOST POPULAR DISHES, CHOSEN BY THE PEOPLE</h2>\r\n\t\t\t\t<p>Believe it or not, eating healthy can be tasty too. Our dishes are carefully crafted to check all the essential macro boxes without sacrificing flavor.</p>\r\n\r\n\t\t\t\t<a href=\"../meals/\" class=\"ob-btn ob-btn-inline\">Shop Popular Meals</a>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"col-md-6 col-sm-6 slide-col\">\r\n\t\t\t\t<div class=\"slide-wrapper\">\r\n\t\t\t\t\t<div class=\"image-slide\">\r\n\t\t\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/caveman-steak.png'}}\" alt=\"\" />\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"image-slide\">\r\n\t\t\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/turkey-pasta.png'}}\" alt=\"\" />\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"image-slide\">\r\n\t\t\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/shredded-bbq.png'}}\" alt=\"\" />\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-new-meal-panel": "<style>.ob-btn-blue:hover {\r\n    background-color: #005677!important;\r\n}\r\n.ob-content-panel.ob-new-meal-panel {\r\n    background: linear-gradient( \r\n315deg\r\n , rgba(0,88,119,1) 0%, rgba(46,179,118,1) 100%)!important;\r\n}\r\n\r\n</style>\r\n\r\n<div class=\"ob-content-panel ob-new-meal-panel\" style=\"background:#64be6b\">\r\n\t<div class=\"container\">\r\n\t\t<div class=\"snack-slider-wrapper\">\r\n\t\t\t<div class=\"snack\">\r\n\t\t\t\t<div class=\"row\">\r\n\t\t\t\t\t<div class=\"col-md-6 col-sm-6 item-content-col\">\r\n\t\t\t\t\t\t<h4 class=\"ob-heading-sm\" style=\"color:white\">NEW SNACK ALERT!</h4>\r\n\t\t\t\t\t\t<h2 class=\"ob-heading-lg\" style=\"color:white\">GLUTEN-FREE MACADAMIA NUT PROTEIN COOKIE</h2>\r\n\t\t\t\t\t\t<p style=\"color:white\">Gluten free, high protein cookie baked with keto flour and spotted with decadent white chocolate and macadamia nuts.</p>\r\n\t\t\t\t\t\t<p class=\"quote\" style=\"color:white\">“SOOO DELICIOUS & GUILT FREE”<br />– KAYLA G.</p>\r\n\t\t\t\t\t\t<a class=\"ob-btn ob-btn-blue ob-btn-inline\" href=\"../snacks/\" style=\"color:#fa8a00!important; \">Shop Fit Snacks</a>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"col-md-2 col-sm-2 item-stats-col\">\r\n\t\t\t\t\t\t<p class=\"ob-paragraph-xl\" style=\"color:white\">7 g <span style=\"color:white\">PROTEIN</span></p>\r\n\t\t\t\t\t\t<p class=\"ob-paragraph-xl\" style=\"color:white\">10 g <span style=\"color:white\">CARBS</span></p>\r\n\t\t\t\t\t\t<p class=\"ob-paragraph-xl\" style=\"color:white\">0 g <span style=\"color:white\">ADDED SUGAR</span></p>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"col-md-4 col-sm-4 item-image-col\">\r\n\t\t\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/cookie.png'}}\" alt=\"\" />\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"snack\">\r\n\t\t\t\t<div class=\"row\">\r\n\t\t\t\t\t<div class=\"col-md-6 col-sm-6 item-content-col\">\r\n\t\t\t\t\t\t<h4 class=\"ob-heading-sm\" style=\"color:white\">NEW MEAL ALERT!</h4>\r\n\t\t\t\t\t\t<h2 class=\"ob-heading-lg\" style=\"color:white\">FARM RAISED FRESH SALMON WITH BROCCOLI</h2>\r\n\t\t\t\t\t\t<p style=\"color:white\">The best Atlantic salmon served with nutrient-dense organic broccoli florets on a bed of our famous gluten-free brown rice.</p>\r\n\t\t\t\t\t\t<p class=\"quote\" style=\"color:white\">“Clean. Delicious. High-Protein.”</p>\r\n\t\t\t\t\t\t<a class=\"ob-btn ob-btn-blue ob-btn-inline\" href=\"../meals/\" style=\"color:#fa8a00!important\">Shop New Meals</a>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"col-md-2 col-sm-2 item-stats-col\">\r\n\t\t\t\t\t\t<p class=\"ob-paragraph-xl\" style=\"color:white\">25 g <span style=\"color:white\">PROTEIN</span></p>\r\n\t\t\t\t\t\t<p class=\"ob-paragraph-xl\" style=\"color:white\">23 g <span style=\"color:white\">CARBS</span></p>\r\n\t\t\t\t\t\t<p class=\"ob-paragraph-xl\" style=\"color:white\">365 <span style=\"color:white\">CALORIES</span></p>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"col-md-4 col-sm-4 item-image-col\">\r\n\t\t\t\t\t\t<img src=\"{{cdn 'assets/img/ob-custom/salmon_3.png'}}\" alt=\"\" />\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
  "components/ob-custom/home/ob-social-embed": "<div class=\"ob-content-panel ob-social-embed\">\r\n\t<div class=\"container\">\r\n\t\t<div class=\"social-content\">\r\n\t\t\t<h2 class=\"ob-heading-lg\">JOIN THE FIT FAM ON SOCIAL </h2>\r\n\t\t\t<p class=\"ob-paragraph-lg\"><a href=\"https://www.instagram.com/myfitfoodsofficial/\">Follow us on IG</a> for exclusive deals and nutritional pro tips from Dr. House</p>\r\n\t\t\t<div id=\"papainstagram-render\"></div>\r\n\t\t</div>\r\n\t</div>\r\n</div>\r\n",
