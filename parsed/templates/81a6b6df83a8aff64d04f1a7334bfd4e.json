{"components/ob-custom/blog/ob_blog-post":"<article class=\"blog col-md-6 col-sm-6\">\r\n    <div class=\"blog-post-wrapper\">\r\n        <div class=\"blog-post-figure\">\r\n            {{#if post.thumbnail}}\r\n            <a href=\"{{post.url}}\">\r\n                <img src=\"{{getImage thumbnail 'gallery' 'blog_size-new'}}\" alt=\"{{thumbnail.alt}}\">\r\n            </a>\r\n            {{else}}\r\n            <a href=\"{{post.url}}\" class=\"no-thumbnail\">\r\n                <img src=\"{{cdn 'assets/img/ob-custom/MFF_Website_Files_Logo_Nav.png'}}\" alt=\"\" />\r\n            </a>\r\n            {{/if}}\r\n        </div>\r\n\r\n        {{#if post.tags}}\r\n        <ul class=\"tags\">\r\n            {{#each post.tags}}\r\n            <li class=\"tag\">\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n        {{/if}}\r\n\r\n        <div class=\"blog-post-body\">\r\n            <header class=\"blog-header\">\r\n                <h2 class=\"ob-heading-sm\"><a href=\"{{post.url}}\">{{post.title}}</a></h2>\r\n                <p class=\"blog-date\">Posted {{post.date_published}}</p>\r\n            </header>\r\n        </div>\r\n    </div>\r\n</article>\r\n"}
