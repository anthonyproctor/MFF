{
  "components/ob-custom/ob-header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\r\n{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n    {{#each (limit banners.top_metadata 1)}}\r\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n        <div data-event-type=\"promotion-click\">\r\n            {{{this.content}}}\r\n        </div>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n<header class=\"header\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n    </a>\r\n\r\n    <div class=\"container header-container\">\r\n        {{#if page_type '===' 'default'}}\r\n            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                <a href=\"/\"><img src=\"{{cdn 'assets/img/ob-custom/logo-blue.png'}}\" alt=\"\" /></a>\r\n            </h1>\r\n        {{else}}\r\n            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n                <a href=\"/\"><img src=\"{{cdn 'assets/img/ob-custom/logo-blue.png'}}\" alt=\"\" /></a>\r\n            </div>\r\n        {{/if}}\r\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n            {{> components/ob-custom/ob-navigation-menu}}\r\n        </div>\r\n        <div class=\"navPages-quickSearch ob-desktop\">\r\n            {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n        </div>\r\n    </div>\r\n</header>\r\n{{{region name=\"header_bottom--global\"}}}\r\n{{{region name=\"header_bottom\"}}}\r\n\r\n<a href=\"\" class=\"skip-to-top\">\r\n    <svg>\r\n        <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-arrow-up\"></use>\r\n    </svg>\r\n</a>\r\n",
  "components/ob-custom/ob-navigation-menu": "<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch ob-mobile\">\r\n      {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action\"\r\n               href=\"/quickstart\"\r\n               aria-label=\"Shop\">\r\n                Shop\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Why MFF?' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/why-mff\"\r\n               aria-label=\"Why MFF?\">\r\n                Why MFF?\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Fit Club' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/fit-club\"\r\n               aria-label=\"Fit Club\">\r\n                Fit Club\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if '21 Days' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/21-day-challenge\"\r\n               aria-label=\"21 Days\">\r\n                21 Days\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Locations' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/locations\"\r\n               aria-label=\"Locations\">\r\n                Locations\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action{{#if 'Blog' '==' ../page.title}} activePage{{/if}}\"\r\n               href=\"/blog\"\r\n               aria-label=\"Blog\">\r\n                Blog\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page navPages-item-icon\">\r\n            {{#if customer}}\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   aria-label=\"{{lang 'common.account'}}\"\r\n                >\r\n                 <svg><use xlink:href=\"#icon-account\" /></svg>\r\n                </a>\r\n            {{else}}\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                <svg class=\"ob-desktop\"><use xlink:href=\"#icon-account\" /></svg>\r\n                <span class=\"ob-mobile\">Account</span>\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page navPages-item-icon\">\r\n            <a class=\"navPages-action\"\r\n               href=\"{{urls.cart}}\"\r\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n                <span class=\"navUser-item--sub navUser-item-cartLabel ob-desktop\">          \r\n                  <svg><use xlink:href=\"#icon-ob-basket\" /></svg>              \r\n                </span>\r\n                <span class=\"navUser-item--sub full-cart-qty ob-desktop\">(<span class=\"countPill cart-quantity\"></span>)</span>\r\n                <span class=\"ob-mobile\">\r\n                  View Cart\r\n                  <span class=\"navUser-item--sub full-cart-qty\">(<span class=\"countPill cart-quantity\"></span>)</span>\r\n                </span>\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page navPages-item-search navPages-item-icon\">\r\n          <a class=\"navPages-action ob-search-toggle\">\r\n            <svg>\r\n              <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-ob-search\"></use>\r\n            </svg>\r\n          </a>\r\n        </li>\r\n    </ul>\r\n</nav>\r\n",
  "components/common/quick-search": "<div class=\"container\">\r\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                       data-search-quick\r\n                       name=\"{{name}}\"\r\n                       id=\"{{name}}\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\r\n                       autocomplete=\"off\"\r\n                >\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <span class=\"exit\">x</span>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n    <p role=\"status\"\r\n       aria-live=\"polite\"\r\n       class=\"aria-description--hidden\"\r\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\r\n    ></p>\r\n</div>\r\n"
}
